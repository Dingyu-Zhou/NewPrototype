{
  "name": "wisdomile-client",
  "version": "0.1.0",
  "description": "WisdoMile Client",
  "author": "All WisdoMile Contributors",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test-react": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "style-fix": "standard --fix \"./src/**/*.js\"",
    "test": "jest",
    "test-changed": "jest -o",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "doc": "esdoc"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./test_helpers/main.js",
    "coverageDirectory": "./generated/test_coverage",
    "coveragePathIgnorePatterns": [
      "./__mocks__",
      "./node_modules/",
      "./generated/",
      "./public",
      "./test_helpers"
    ]
  },
  "standard": {
    "env": "jest"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.5",
    "apollo-client": "^2.2.0",
    "apollo-link-http": "^1.3.2",
    "core-js": "^2.5.1",
    "draft-js": "^0.10.4",
    "draft-js-plugins-editor": "^2.0.1",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.1",
    "immutable": "^3.8.2",
    "prop-types": "^15.5.10",
    "raf": "^3.4.0",
    "react": "^16.1.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.1.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-persist": "^5.3.4",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "casual": "^1.5.17",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "esdoc": "^1.0.3",
    "esdoc-jsx-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "jest": "^21.2.1",
    "react-scripts": "^1.0.17",
    "react-test-renderer": "^16.1.0",
    "standard": "^10.0.3"
  }
}
